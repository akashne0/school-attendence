<b>Id:</b>
  <%= link_to user.id, user_path(user) %>
<br>
<b>Email:</b>
  <%= user.email %>
<br>
<b>Password:</b>
  <%= user.password %>
<br>
<b>Confirmation:</b>
  <%= user.confirmed? %>
<br>
<b>Sign_in count:</b>
  <%= user.sign_in_count %>
<br>
<b>Current sign_in at:</b>
  <%= user.current_sign_in_at  %>
<br>
<b>Last sign_in at:</b>
  <%= user.last_sign_in_at %>
<br>
<b>Current sign_in IP:</b>
  <%= user.current_sign_in_ip %>
<br>
<b>Last sign_in IP:</b>
  <%= user.last_sign_in_ip %>
<br>
<b>Locked at:</b>
  <%= user.locked_at %>
<br>
<%= link_to "Delete", user, method: :delete, data: {confirm: "Are You Sure?"}%> |
<%= link_to ban_status(user), ban_user_path(user), method: :patch %>
